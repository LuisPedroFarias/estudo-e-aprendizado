programa {
  funcao inicio() {
    real numero

    escreva("Digite um numero:")
    leia(numero)

      se (numero == 10) {
        escreva("É IGUAL A 10")
      }
      
      se (numero > 10) {
        escreva("É MAIOR QUE 10!")
      }

      se (numero < 10) {
        escreva("É MENOR QUE 10!")
      } 

      escreva("\n=== FIM DO PROGRAMA ===")

  }
}
